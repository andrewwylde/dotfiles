[init]
  defaultBranch = main
  templatedir = ~/.git_template
[push]
  default = current
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
	editor = /usr/bin/vim
[commit]
  template = ~/.gitmessage
	gpgsign = false
[fetch]
  prune = true
[rebase]
  autosquash = true
	autoStash = true
[include]
  path = ~/.gitconfig.local
[diff]
  colorMoved = zebra
[pull]
	rebase = true
