[init]
  defaultBranch = main
  templatedir = ~/.git_template
[push]
  default = current
[color]
  ui = auto
[core]
  autocrlf = input
	editor = /usr/bin/vim
[commit]
  template = ~/.gitmessage
	gpgsign = true
[fetch]
  prune = true
[rebase]
  autosquash = true
[include]
  path = ~/.gitconfig.local
[diff]
  colorMoved = zebra
[user]
	signingkey = 400D9D65916C2386
