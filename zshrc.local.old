# Path to your oh-my-zsh installation.
export ZSH=/home/drewkakes/.oh-my-zsh

plugins=(git)


# User configuration

export PATH="/home/drewkakes/.nvm/versions/node/v4.2.6/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:$PATH"
# export MANPATH="/usr/local/man:$MANPATH"

source $ZSH/oh-my-zsh.sh

alias grut="grunt jshint && grunt eslint"

export NVM_DIR="/home/drewkakes/.nvm"
